<html>

<head></head>

<body>
    <div align="center">
        <h1>Local Chat App</h1>
    </div>
    <div style="height:20px; background-color:purple; display: inline-block; padding:10">
        <div style="display: inline-block;">
            <input id="username" type="text" name="username">
        </div>
        <div style="display: inline-block;">
            <button id="send_username" type="button">Change username</button>
        </div>
    </div>
    <div id="chatroom" style="height:80%; background-color:beige">

    </div>
    <div style="height:20px; background-color:purple; padding:10">
        <span>
            <input id="message" style="width:95%" type="text" name="username">
        </span>
        <div style="display: inline-block;">
            <button id="send_message" type="button">Send</button>
        </div>
    </div>


</body>
<script src="http://code.jquery.com/jquery-latest.min.js"></script>
<script src="chat.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.0.4/socket.io.js"></script>

</html>